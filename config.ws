
keys {
	alt+up,alt+k 					workspaceNext
	alt+down,alt+l 					workspacePrev
	alt+shift+down,alt+shift+k 		moveWorkspaceNext
	alt+shift+up,alt+shift+l		moveWorkspacePrev
	alt+j 							focusLeft
	alt+; 							focusRight
	alt+d 							exec dinu -f Consolas-10 -l 20 -c ~/.dinu/$WS
	alt+shift+q 					killClient
	alt+shift+e 					quit
}

